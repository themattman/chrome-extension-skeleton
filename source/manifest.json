{
    "name": "Link Rewriter",
    "description": "Redirect Youtube links to Youtubeonrepeat",
    "version": "0.1",
    "manifest_version": 2,
    "content_scripts": [
	{
	    "matches": [
		"https://www.youtube.com/*"
	    ],
	    "js": [
		"redirecter.js"
	    ]
	}
    ],
    "background": {
	"scripts": [
	    "background.js"
	]
    }
}
